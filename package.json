{
  "name": "Three(js)D",
  "author": "Keith Showalter",
  "version": "0.1.0",
  "main": "server/server.js",
  "description": "Test of three.js",
  "scripts": {
    "serve": "nodemon server/server.js",
    "webpack": "webpack --progress --colors --watch -d",
    "webpack_serve": "webpack-dev-server --content-base public/ --inline --hot -d",
    "watchify": "watchify main.js -o index.js -d -v",
    "babelify": "watchify -t babelify main.js -o index.js -d -v",
    "babel": "babel main.es6 --watch --out-file index.js --source-maps"
  },
  "dependencies": {
    "brfs": "latest",
    "chance": "latest",
    "chart_maker": "git+https://github.com/kshowalter/chart_maker.git",
    "express": "latest",
    "freezer-js": "latest",
    "hash_router": "https://github.com/kshowalter/hash_router",
    "jquery": "latest",
    "lodash": "^4.11.2",
    "lowdb": "latest",
    "math": "latest",
    "mkdrawing": "git+https://github.com/kshowalter/mkDrawing.git",
    "moment": "latest",
    "notification-logger": "latest",
    "passport": "latest",
    "passport-local": "latest",
    "redux": "latest",
    "redux-logger": "latest",
    "redux-thunk": "latest",
    "seedrandom": "latest",
    "simple_indexer": "git+https://github.com/kshowalter/simple_indexer.git",
    "socket.io": "latest",
    "socket.io-client": "latest",
    "specdom": "git+https://github.com/kshowalter/specdom.git",
    "specdom_helper": "git+https://github.com/kshowalter/specDOM_helper.git",
    "specdom_to_sgml": "git+https://github.com/kshowalter/specDOM_to_SGML.git",
    "svgize": "git+https://github.com/kshowalter/svgize.git",
    "three": "latest",
    "three-js-csg": "^72.0.0"
  },
  "devDependencies": {
    "babel-core": "6.1.21",
    "babel-eslint": "latest",
    "babel-loader": "latest",
    "babel-preset-es2015": "latest",
    "eslint": "latest",
    "webpack": "latest",
    "webpack-dev-server": "latest"
  },
  "jshintConfig": {
    "node": true,
    "browser": true,
    "esnext": true,
    "funcscope": true,
    "sub": true,
    "globals": {
      "g": true,
      "$": true,
      "_": true,
      "moment": true,
      "math": true
    }
  }
}
