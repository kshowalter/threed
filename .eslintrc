{
  "rules": {
    "indent": [
      1,
      2
    ],
    "quotes": [
      1,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-console": 0,
    "no-unused-vars": 1,
  },
  "env": {
    "node": true,
    "browser": true
  },
  "extends": "eslint:recommended",

  "parser": "babel-eslint",
  "globals": {
    "$": true,
    "_": true,
  }

}
